class EnhancedFor {
    public static void main(String[] args) {
        int[] ar={1,2,3,4,5};

        for(int e: ar){ // 배열 요소 전체 출력
            System.out.println(e + " ");
        }
        System.out.println();   //단순 줄 바꿈 목적

        int sum=0;

        for(int e: ar){ //배열 요소의 전체 합 출력
            sum+=e;
        }
        System.out.println("sum: " + sum);
    }
}
