public class Main {
	public static void main(String[] args) {
		int i,j;
		System.out.println("구구단을 계산하자!");
		for(i=2; i<=9; i++) {
			System.out.println("-------"+i+"단--------");
			for(j=1; j<=9; j++) {
				System.out.print("[" + i+"x"+j+"]"+"="+(i*j) );
				System.out.print('\t');
			}
			System.out.println('\n');
		}
	}
}
